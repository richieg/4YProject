myfunct =function(){
    	
        alert('button clicked');
        var fname=$("#firstName").val();
        alert(fname);
        var lname=$("#lastName").val();
        var address1=("#address1").val();
        $.ajax({
        	url: "/src/AddUsers",
        	data: {
        	fname: fname,
        	lname: lname,
        	address: address1
        	},
        	success: function( data ) {
        	$( "#alertmessage" ).html( "<strong>" + data + "</strong> degrees" );
        	}
        	});
    	};
  